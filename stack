import java.util.*;
class Node{
  int sum;
  Node next;
}
class Main
{
    public static void push(Node head)
    {
        Node ob = new Node();
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        ob.sum = n;
        ob.next = head.next;
        head.next = ob;
    }
    
    public static void pop(Node head)
    {
        if(head.next!=null)
        {
            head.next = head.next.next;
            
        }
    }
    
    public static void peek(Node head)
    {
        if(head.next!=null)
        {
            System.out.println(head.next.sum);
        }
    }
    
    public static void size(Node head)
    {
        int c = 0;
        Node t = head.next;
        while(t!=null)
        {
            c++;
            t = t.next;
        }
        System.out.println("size:"+c);
    }
    
    public static void display(Node head)
    {
        Node t = head.next;
        while(t!=null)
        {
            System.out.print(t.sum+"  ");
            t = t.next;
        }
    }
    
    
    public static void main(String args[])
    {
        Node head = new Node();
        head.next = null;
        Scanner sc=new  Scanner(System.in);
        int n=sc.nextInt();
        for(int i=0;i<n;i++)
        {
            push(head);
        }

        pop(head);
        peek(head);
        size(head); 
        display(head); 
    }
    
}
